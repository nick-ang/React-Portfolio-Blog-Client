   this.setState({\n        errorMessage: `Failed to ${isCreate ? \"create\" : \"update\"} record: ${\n          result.status\n        } ${result.statusText}`,\n      });\n    } else {\n      this.setState({ errorMessage: null });\n      this.props.history.push(\"/articles\");\n    }\n  }\n\n  render() {\n    const { item, errorMessage, isCreate } = this.state;\n    const title = <h2>{isCreate ? \"Add Article\" : \"Edit Article\"}</h2>;\n\n    return (\n      <div>\n        {this.props.navbar}\n        <Container style={{ textAlign: \"left\" }}>\n          {title}\n          <br />\n          {errorMessage ? <Alert color=\"warning\">{errorMessage}</Alert> : null}\n          <Form onSubmit={this.handleSubmit}>\n            <div className=\"row\">\n              <FormGroup className=\"title col-md-12 mb-3\">\n                <Label for=\"title\">Title</Label>\n                <Input\n                  type=\"hidden\"\n                  name=\"title\"\n                  id=\"title\"\n                  value={item.title || \"\"}\n                  onChange={this.handleChange}\n                  autoComplete=\"title\"\n                  readonly\n                />\n                <Editor\n                  apiKey=\"emehi8uy1oa5o8lchnrih1issh91tfh2wn2l3iul0tjhr1eo\"\n                  value={this.state.content}\n                  textareaName=\"title\"\n                  value={item.title || \"\"}\n                  init={{\n\n                    height: 300,\n                    menubar: true,\n                  }}\n                  onEditorChange={this.handleChange}\n                />\n              </FormGroup>\n              <FormGroup className=\"col-md-12 mb-3\">\n                <Label for=\"content\">Content</Label>\n                <Input\n                  name=\"content\"\n                  id=\"content\"\n                  value={item.content || \"\"}\n                  onChange={this.handleChange}\n                  autoComplete=\"phone\"\n                  readonly\n                  type=\"hidden\"\n                />\n                <Editor\n                  id=\"content\"\n                   value={item.content || \"\"}\n                  textareaName=\"content\"\n                  init={{\n\n                    height: 500,\n                    menubar: true,\n                  }}\n                  onEditorChange={this.handleChange}\n                />\n              </FormGroup>\n            </div>\n            <FormGroup>\n              <Button color=\"primary\" type=\"submit\">\n                Save\n              </Button>{\" \"}\n              <Button color=\"secondary\" tag={Link} to=\"/articles\">\n                Cancel\n              </Button>\n            </FormGroup>\n          </Form>\n        </Container>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(ArticleEdit);\n","/Users/nicholasang/Documents/blog/src/Home.js",[],"/Users/nicholasang/Documents/blog/src/Api.js",[],"/Users/nicholasang/Documents/blog/src/ArticlesList.js",["191","192","193"],"import React, { Component } from \"react\";\nimport { Alert, Button } from \"reactstrap\";\nimport Lottie from \"react-lottie-player\";\nimport { Link } from \"react-router-dom\";\nimport ReactHtmlParser, {\n  processNodes,\n  convertNodeToElement,\n  htmlparser2,\n} from \"react-html-parser\";\nimport bear from \"./animations/bear3.json\";\nimport AOS from \"aos\";\nimport \"aos/dist/aos.css\";\n\n\nconst Article = (props) => (\n\n  <div data-aos=\"fade-up\" className=\"article-container rounded-3xl p-2 m-2\">\n    <h3>{props.name}</h3>\n    <div className=\"article-body\">\n      <div className=\"bg-white rounded-3xl subtitle-container\">\n        <div className=\"bg-black rounded-t-3xl\">\n          <br />\n          <br />\n          <h2 className=\"title-font sm:text-4xl text-3xl font-medium text-center text-white\">{ReactHtmlParser(props.title)}</h2>\n          <br />\n           <br />\n        </div>\n        <div className=\"blog p-10\">{ReactHtmlParser(props.content)}</div>\n      </div>\n    </div>\n    <div className=\"article-footer\">\n      <Button\n        classname=\"w-full\"\n        color=\"secondary\"\n        tag={Link}\n        to={\"/articles/\" + props.id}\n      >\n        Edit\n      </Button>\n      <Button color=\"danger\" onClick={() => props.remove(props.id)}>\n        Delete\n      </Button>\n      <Button color=\"success\" tag={Link} to=\"/articles/new\">\n        Add New\n      </Button>\n    </div>\n  </div>\n);\n\nclass ArticlesList extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      articles: [],\n      isLoading: true,\n      errorMessage: null,\n    };\n    this.remove = this.remove.bind(this);\n  }\n\n  async componentDidMount() {\n    this.setState({ isLoading: true });\n    const response = await this.props.api.getAll();\n    if (!response.ok) {\n      this.setState({\n        errorMessage: `Failed to load: ${response.status} ${response.statusText}`,\n        isLoading: false,\n      });\n    } else {\n      const body = await response.json();\n      const articles = body;\n      this.setState({\n        articles: articles,\n        isLoading: false,\n        errorMessage: null,\n      });\n    }\n  }\n\n  async remove(id) {\n    let response = await this.props.api.delete(id);\n    if (!response.ok) {\n      this.setState({\n        errorMessage: `Failed to delete article: ${response.status} ${response.statusText}`,\n      });\n    } else {\n      let updatedArticles = [...this.state.articles].filter((i) => i.id !== id);\n      this.setState({ articles: updatedArticles, errorMessage: null });\n    }\n  }\n\n  render() {\n    AOS.init();\n    const { articles, isLoading, errorMessage } = this.state;\n\n    if (isLoading) {\n      return <p>Loading...</p>;\n    }\n\n    return (\n      <div>\n        {this.props.navbar}\n        <div className=\" justify-content-between\">\n        <br/>\n        <br/>\n        <h1 className=\"title-font lg:text-5xl text-4xl mb-4 font-medium text-center text-white\">\n          Blog Articles ðŸ“š\n          </h1>\n          <br />\n          <p className=\"text-white text-center\">\n            <em>\n              \"Just a place for me to keep a collection of my personal thoughts\n              and a record of my\n              journey through life. Enjoy your time here!\"\n            </em>\n          </p>\n          <div className=\"container w-full flex justify-center\">\n            <div className=\"w-25 content-center\">\n              <Lottie loop animationData={bear} play />\n            </div>\n          </div>\n        </div>\n        {errorMessage ? (\n          <div className=\"d-flex flex-row justify-content-center\">\n            <Alert color=\"warning\" style={{ flex: 1, maxWidth: \"80%\" }}>\n              {errorMessage}\n            </Alert>\n          </div>\n        ) : null}\n        <div className=\"d-flex flex-row flex-container flex-wrap justify-content-center\">\n          {articles.map((article) => (\n            <Article\n              {...article}\n              remove={this.remove.bind(this)}\n              key={article.id}\n            />\n          ))}\n          {!articles || articles.length === 0 ? <p>No articles!</p> : null}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default ArticlesList;\n","/Users/nicholasang/Documents/blog/src/NavBar.js",[],"/Users/nicholasang/Documents/blog/src/components/About.js",[],"/Users/nicholasang/Documents/blog/src/components/Contact.js",[],"/Users/nicholasang/Documents/blog/src/components/Skills.js",[],"/Users/nicholasang/Documents/blog/src/components/Projects.js",[],"/Users/nicholasang/Documents/blog/src/components/Testimonials.js",[],"/Users/nicholasang/Documents/blog/src/data.js",[],"/Users/nicholasang/Documents/blog/src/components/Footer.js",["194","195","196","197","198"],"import React from \"react\";\nimport { MDBCol, MDBContainer, MDBRow, MDBFooter } from \"mdbreact\";\n\ncons